{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "require": {
    "laravel/framework": "4.2",
    "zizaco/confide": "3.2.x",
    "zizaco/entrust": "1.2.5",
    "laravelbook/ardent": "2.5.0",
    "andrew13/cabinet": "1.1.4.",
    "summerstreet/woodling": "dev-master",
    "balanced/balanced": "1.x",
    "lusitanian/oauth": "~0.3",
    "doctrine/dbal": "2.5.4.0@beta",
    "webignition/internet-media-type": "dev-master",
    "thujohn/twitter": "dev-master",
    "felixkiss/uniquewith-validator": "1.1.*",
    "way/generators": "2.*",
    "fzaninotto/faker": "dev-master",
    "facebook/php-sdk-v4": "4.0.9",
    "guzzlehttp/guzzle": "4.2.3",
    "pda/pheanstalk": "~2.0",
    "openlss/lib-array2xml": "dev-master",
    "ise/php-soundcloud": "3.0.1",
    "dropbox/dropbox-sdk": "1.1.*",
    "eher/oauth": "1.0.*",
    "google/apiclient": "1.1.2",
    "traackr/traackr-api-php": "0.15.1",
    "vimeo/vimeo-api": "dev-master",
    "orchestra/imagine": "2.2.1",
    "aweber/aweber": "~1.1.12",
    "indatus/dispatcher": "1.*",
    "soundasleep/html2text": "~0.2",
    "barryvdh/laravel-dompdf": "0.4.*",
    "phpunit/phpunit": "4.8",
    "brianium/paratest": "dev-master",
    "stripe/stripe-php": "^3.7"
  },
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/models",
      "app/database/migrations",
      "app/database/seeds",
      "app/tests/TestCase.php",
      "app/libraries"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}
