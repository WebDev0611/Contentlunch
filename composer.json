{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/jbizzay/cabinet"
    },
    {
      "type": "vcs",
      "url": "https://github.com/surgeforward/laravel-resque.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/surgeforward/php-resque-scheduler.git"
    }
  ],
  "require": {
    "laravel/framework": "4.1.*",
    "laravelbook/ardent": "dev-master",
    "zizaco/confide": "dev-master",
    "zizaco/entrust": "dev-master",
    "andrew13/cabinet": "dev-master",
    "summerstreet/woodling": "dev-master",
    "balanced/balanced": "1.x",
    "lusitanian/oauth": "~0.3",
    "doctrine/dbal": "~2.3",
    "webignition/internet-media-type": "dev-master",
    "happyr/linkedin-api-client": "dev-master",
    "thujohn/twitter": "dev-master",
    "felixkiss/uniquewith-validator": "1.1.*",
    "way/generators": "2.*",
    "fzaninotto/faker": "dev-master",
    "facebook/php-sdk-v4": "4.0.9",
    "guzzlehttp/guzzle": "dev-master",
    "chrisboulton/php-resque": "dev-master",
    "chrisboulton/php-resque-scheduler": "dev-actually-works",
    "awellis13/laravel-resque": "dev-better-laravel"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
      "app/libraries"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}
